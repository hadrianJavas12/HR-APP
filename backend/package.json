{
  "name": "hr-manhour-backend",
  "version": "1.0.0",
  "description": "HR Man-Hour Performance Monitoring System - Backend",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon --experimental-specifier-resolution=node src/index.js",
    "start": "node src/index.js",
    "migrate": "knex migrate:latest --knexfile src/config/knexfile.js",
    "migrate:rollback": "knex migrate:rollback --knexfile src/config/knexfile.js",
    "migrate:make": "knex migrate:make --knexfile src/config/knexfile.js",
    "seed": "knex seed:run --knexfile src/config/knexfile.js",
    "seed:make": "knex seed:make --knexfile src/config/knexfile.js",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.12.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.0.7",
    "nodemailer": "^6.9.14",
    "objection": "^3.1.4",
    "papaparse": "^5.4.1",
    "pg": "^8.12.0",
    "pino": "^9.3.2",
    "pino-pretty": "^11.2.2",
    "socket.io": "^4.7.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^10.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "eslint": "^9.6.0",
    "globals": "^15.8.0",
    "jest": "^29.7.0",
    "@jest/globals": "^29.7.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0"
  }
}
